{
    "name": "zipzoft/cloaking-skeleton",
    "description": "A PHP cloaking system for validating and managing visitor access",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.23",
        "mockery/mockery": "^1.6",
        "symfony/string": "^6.0",
        "symfony/console": "^6.0",
        "phpmetrics/phpmetrics": "^2.8",
        "phpunit/php-code-coverage": "^9.2"
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
